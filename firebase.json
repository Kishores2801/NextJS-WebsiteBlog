{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      ".env",
      ".env.local",
      "**/.*",
      "**/node_modules/**",
      "**/.next/**",
      "**/.firebase/**",
      "**/.git/**",
      "**/dist/**",
      "**/build_log.txt",
      "**/lint_log.txt",
      "README.md",
      "src/**/*.test.*"
    ],
    "frameworksBackend": {}
  },
  "apphosting": {
    "backendId": "my-web-app",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ]
  }
}
